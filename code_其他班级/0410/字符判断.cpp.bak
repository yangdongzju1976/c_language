//输入字符，进行判断。
#include <stdio.h>
int main(){
    char c;
    printf("请输入需要判断的字符:");
    c=getchar();
    
	if(c<32)
        printf("这是个控制字符\n");


    if(c>='0'&&c<='9')
        printf("这是个数字\n");


    if(c>='A'&&c<='Z')
        printf("这是个大写字母\n");


    if(c>='a'&&c<='z')
        printf("这是个小写字母\n");
   
    if(!(c<32) &&!(c>='0'&&c<='9')&&!(c>='A'&&c<='Z')&&!(c>='a'&&c<='z'))
		 printf("其他字符\n");
    
	return 0;
}
