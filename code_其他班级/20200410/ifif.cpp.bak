#include<stdio.h>  

int main() 
	{
    int score = 0;
    printf("\n请输入分数:");
    scanf("%d", &score);

    if (score >= 60) { // 下面是嵌套if-else语句
        if (score <= 80) {
            printf("分数大于60小于80，中等水平\n");
        }else if (score > 80 && score < 90) {
            printf("分数大于60小于80，成绩良好\n");
        }else{// 大于 90 以上
            printf("分数大于90，成绩优秀\n");
        }
    }else 
		if(score>=30)
		{
        printf("大于30分，小于 60 分，不及格，较差   \n");
    }
	else 
	{
		printf("\n低于30分，极差！");
	}
	return 0;
}